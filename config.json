{
  "name": "Pipeline HTML to PDF",
  "desc": "Pipeline running HTML to PDF command.",
  "version": {
    "major": 1,
    "minor": 0
  },
  "pipeline": [
    {
      "name": "HTML to PDF",
      "desc": "Creating PDF document from HTML.",
      "path": "user-defined command folder",
      "program": "html_to_pdf",
      "category": "Conversion",
      "stdout": "",
      "stderr": "",
      "returnCodes": [
        0
      ],
      "configurations": [
        {
          "name": "Convert url HTML to PDF.",
          "desc": "Create PDF document from HTML url address.",
          "args": [
            "---url",
            "${URL}",
            "--output",
            "${Output PDF}"
          ]
        },
        {
          "args": [
            "--input",
            "${Input File}",
            "--output",
            "${Output PDF}"
          ],
          "name": "Convert index file HTML to PDF.",
          "desc": "Create PDF document from HTML index file."
        }
      ],
      "configuration": {
        "name": "Convert HTML to PDF.",
        "desc": "Create PDF document from HTML.",
        "args": [
          "---url",
          "${URL}",
          "--output",
          "${Output PDF}"
        ]
      },
      "args": [
        {
          "name": "URL",
          "desc": "URL address.",
          "flags": 2,
          "type": "string",
          "value": "user-defined url address"
        },
        {
          "name": "Input File",
          "desc": "HTML index file.",
          "ext": "html",
          "type": "file_path",
          "flags": 2,
          "value": "user-defined file path"
        },
        {
          "name": "Output PDF",
          "desc": "Output PDF file.",
          "flags": 4,
          "ext": "pdf",
          "type": "file_path_save",
          "value": "user-defined file path"
        }
      ]
    }
  ]
}