{
  "name": "Pipeline HTML to PDF",
  "desc": "Pipeline running HTML to PDF command.",
  "version": {
    "major": 1,
    "minor": 0
  },
  "pipeline": [
    {
      "name": "HTML to PDF",
      "desc": "Creating PDF file from HTML.",
      "path": "user-defined command folder",
      "program": "html_to_pdf",
      "stdout": "",
      "stderr": "",
      "returnCodes": [
        0
      ],
      "configuration": [
        {
          "name": "Convert HTML to PDF.",
          "desc": "Run VeraPDF to validate PDF document.",
          "args": [
            "---url",
            "${URL}",
            "--output",
            "${Output}"
          ]
        }
      ],
      "args": [
        {
          "name": "URL",
          "desc": "URL address.",
          "type": "string",
          "value": "user-defined url address"
        },
        {
          "name": "Output",
          "desc": "Output PDF file.",
          "type": "file_path_save",
          "value": "user-defined file path"
        }
      ]
    }
  ]
}