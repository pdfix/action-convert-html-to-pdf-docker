{
  "version": {
    "major": 0,
    "minor": 2,
    "patch": 0
  },
  "plug-in": {
    "type": "custom",
    "name": "html_to_pdf",
    "cmd": "html_to_pdf",
    "title": "HTML to PDF",
    "desc": "Creating PDF file from HTML.",
    "path": "${PLUGIN_FOLDER}",
    "args": [
      {
        "title": "URL",
        "desc": "URL address.",
        "name": "--url",
        "value": "${URL}"
      },
      {
        "title": "Output",
        "desc": "Output file.",
        "name": "--output",
        "type": "file_path",
        "value": "${OUTPUT_FILE}"
      }
    ]
  }
}